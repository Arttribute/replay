{
  "name": "@arttribute/eaa-types",
  "version": "0.0.1",
  "description": "Entity • Action • Attribution type definitions and Zod schemas",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
